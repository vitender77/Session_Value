private static string _SchoolID = "_SchoolID";
        public static string SchoolID
        {
            get
            {
                return HttpContext.Current.Session[_SchoolID] == null ? String.Empty : HttpContext.Current.Session[_SchoolID].ToString();
            }
            set
            {
                HttpContext.Current.Session[_SchoolID] = value;
            }
        }
        private static string _SessionID = "_SessionID";
